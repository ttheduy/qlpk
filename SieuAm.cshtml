                content: '';
                display: block;
            }

            .ribbon:before {
                width: 0.5em;
                height: 2.05em;
                padding: 0 0 0.5em;
                top: 0;
                right: -0.5em;
                background: gold;
                border-radius: 0 0 0.5em 0;
            }

            .ribbon:after {
                width: 0.4em;
                height: 0.4em;
                background: rgba(0,0,0,0.5);
                bottom: -0.4em;
                right: -0.4em;
