        .card .thumbnail-outer {
            position: relative;
        }

        .thumbnail {
            overflow: hidden;
            position: relative;
            outline: 0;
            box-sizing: content-box;
        }

            .thumbnail::before {
                content: "";
                display: block;
                padding-top: 100%;
            }

            .thumbnail .thumbnail-frame {
                overflow: hidden;
                position: absolute;
                top: 0;
                right: 0;
                bottom: 0;
                left: 0;
                outline: 0;
            }

            .thumbnail .centered {
                position: absolute;
                -webkit-transform: translate(50%,50%);
                -moz-transform: translate(50%,50%);
                -ms-transform: translate(50%,50%);
                -o-transform: translate(50%,50%);
                transform: translate(50%,50%);
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                box-sizing: content-box;
                outline: 0;
                display: block;
            }

            .thumbnail img {
                position: absolute;
                -webkit-transform: translate(-50%,-50%);
                -moz-transform: translate(-50%,-50%);
                -ms-transform: translate(-50%,-50%);
                -o-transform: translate(-50%,-50%);
                transform: translate(-50%,-50%);
                top: 0;
                left: 0;
                height: 100%;
            }

        .ribbon {
            position: absolute;
            padding: 0.1em 3em;
            font-size: 1em;
            font-weight: bold;
